<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}FABOP directory{% endblock %}</title>
        <link rel="icon" href="{{ asset('static/images/favicon.png') }}">
        {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('static/css/_navbar.scss') }}">
        <link rel="stylesheet" href="{{ asset('static/fonts/Staatliches/staatliches.css') }}">
        <link rel="stylesheet" href="{{ asset('static/css/animate.css') }}">
        <link rel="stylesheet" href="{{ asset('static/css/style.css') }}">
        <link rel="stylesheet" href="{{ asset('static/css/fontawesome/all.min.css') }}">
        <link rel="stylesheet" href="{{ asset('static/css/dataTables.bootstrap4.min.css') }}">
        {% endblock %}
    </head>
    <body onload="startTime()">
        {% block body %}{% endblock %}
        {% block javascripts %}
        {{ encore_entry_link_tags('app') }}
        <script src="{{ asset('static/js/jquery.min.js') }}"></script>
        <script src="{{ asset('static/js/popper.min.js') }}"></script>
        <script src="{{ asset('static/js/jquery.dataTables.min.js') }}"></script>
        {# <script src="{{ asset('static/js/dataTables.buttons.min.js') }}"></script> #}
        <script src="{{ asset('static/js/dataTables.bootstrap4.min.js') }}"></script>
        <script src="{{ asset('static/js/scripts.js') }}"></script>
        <script src="{{ asset('static/js/login.js') }}"></script>
        <script src="{{ asset('static/js/bootstrap.min.js') }}"></script>
        {% endblock %}
    </body>
</html>
